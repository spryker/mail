<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xsd.spryker.com ../../src/Spryker/TransferObjectDefinitionSchema.xsd">

    <transfer name="MailAddress">
        <property name="countryName" type="string" default="null" />
    </transfer>
    <transfer name="MailAttachment">
        <property name="fileName" type="string" default="null" />
        <property name="attachmentUrl" type="string" default="null" />
        <property name="referenceId" type="string" default="null" />
    </transfer>
    <transfer name="MailCustomer">
        <property name="customerId" type="string" default="null" />
    </transfer>
    <transfer name="MailItem">
        <property name="idSalesOrderItem" type="int" default="null" />
        <property name="name" type="string" default="null" />
        <property name="sku" type="string" default="null" />
        <property name="grossPrice" type="string" default="null" />
        <property name="totalPrice" type="string" default="null" />
        <property name="quantity" type="string" default="null" />
    </transfer>
    <transfer name="MailMail">
        <property name="referenceId" type="string" default="null" />
        <property name="type" type="string" default="null" />
        <property name="subType" type="string" default="null" />
        <property name="priority" type="string" default="null" />
        <property name="recipientAddress" type="string" default="null" />
        <property name="recipientFullname" type="string" default="null" />
        <property name="senderAddress" type="string" default="null" />
        <property name="senderFullname" type="string" default="null" />
        <property name="replyToAddress" type="string" default="null" />
        <property name="subject" type="string" default="null" />
        <property name="charset" type="string" default="null" />
        <property name="data" type="string" default="null" />
        <property name="isOrderMail" type="string" default="null" />
        <property name="isTestOrder" type="string" default="null" />
        <property name="isUnique" type="string" default="null" />
        <property name="yvesUrl" type="string" default="null" />
        <property name="dateInterval" type="string" default="null" />
        <property name="staticMediaUrl" type="string" default="null" />
        <property name="staticAssetsUrl" type="string" default="null" />
        <property name="attachments" type="array" default="[]" />
    </transfer>
    <transfer name="MailMailTransfer">
        <use name="SprykerFeature\Shared\Library\TransferObject\AbstractTransfer" short="AbstractTransfer" />
        <property name="templateName" type="string" default="null" />
        <property name="templateContent" type="array" default="[]" />
        <property name="async" type="bool" default="false" />
        <property name="ipPool" type="string" default="null" />
        <property name="sendAt" type="string" default="null" />
        <property name="subject" type="string" default="null" />
        <property name="fromEmail" type="string" default="null" />
        <property name="fromName" type="string" default="null" />
        <property name="recipients" type="array" default="[]" />
        <property name="headers" type="array" default="[]" />
        <property name="important" type="bool" default="false" />
        <property name="trackOpens" type="bool" default="false" />
        <property name="trackClicks" type="bool" default="false" />
        <property name="autoText" type="bool" default="false" />
        <property name="autoHtml" type="bool" default="false" />
        <property name="inlineCss" type="bool" default="false" />
        <property name="urlStripQueryString" type="bool" default="false" />
        <property name="preserveRecipients" type="bool" default="false" />
        <property name="viewContentLink" type="bool" default="false" />
        <property name="bccAddress" type="string" default="null" />
        <property name="trackingDomain" type="string" default="null" />
        <property name="signingDomain" type="string" default="null" />
        <property name="returnPathDomain" type="string" default="null" />
        <property name="merge" type="bool" default="false" />
        <property name="mergeLanguage" type="string" default="mailchimp" />
        <property name="globalMergeVars" type="array" default="[]" />
        <property name="recipientMergeVars" type="array" default="[]" />
        <property name="tags" type="array" default="[]" />
        <property name="subAccount" type="string" default="null" />
        <property name="googleAnalyticsDomains" type="array" default="[]" />
        <property name="googleAnalyticsCampaign" type="string" default="null" />
        <property name="metadata" type="array" default="[]" />
        <property name="recipientMetadata" type="array" default="[]" />
        <property name="attachments" type="array" default="[]" />
        <property name="images" type="array" default="[]" />
    </transfer>
    <transfer name="MailTemplate">
        <property name="idMailTemplate" type="int" default="null" />
        <property name="name" type="string" default="null" />
        <property name="subject" type="string" default="null" />
        <property name="sender" type="string" default="null" />
        <property name="senderName" type="string" default="null" />
        <property name="text" type="string" default="null" />
        <property name="html" type="string" default="null" />
        <property name="wrapper" type="int" default="0" />
        <property name="deleted" type="string" default="null" />
        <property name="version" type="string" default="null" />
        <property name="dateInterval" type="string" default="null" />
        <property name="versionCreatedAt" type="string" default="null" />
        <property name="versionCreatedBy" type="string" default="null" />
    </transfer>

</transfers>

